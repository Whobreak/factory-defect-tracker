{
  "expo": {
    "name": "factory-defect-tracker",
    "slug": "factory-defect-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-dark.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/iPhone 16 Frame (1).png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
      "image": "./assets/images/iPhone 16 Frame (1).png",
      "resizeMode": "contain",
      "backgroundColor": "##0f172a"
    }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon2.png",
        "backgroundColor": "##0f172a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"      
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
